    "typescript": "~2.7.2"
  },
  "devDependencies": {
    "@microsoft/node-library-build": "6.3.2",
    "@microsoft/rush-stack-compiler-3.4": "0.3.2",
    "@microsoft/rush-stack-compiler-shared": "0.0.0",
    "@rushstack/eslint-config": "0.4.2",
