{
  "name": "rush-lib-test",
  "version": "0.0.0",
  "private": true,
  "description": "A minimal example project that imports APIs from @rushstack/rush-lib",
  "license": "MIT",
  "scripts": {
    "build": "heft build --clean",
    "start": "node lib/start.js",
    "_phase:build": "heft run --only build -- --clean"
  },
  "dependencies": {
    "@microsoft/rush-lib": "workspace:*",
    "@rushstack/terminal": "workspace:*"
  },
  "devDependencies": {
    "@rushstack/heft": "workspace:*",
    "@types/node": "20.17.19",
    "eslint": "~9.25.1",
    "local-node-rig": "workspace:*"
  },
  "dependenciesMeta": {
    "@microsoft/rush-lib": {
      "injected": true
    },
    "@rushstack/terminal": {
      "injected": true
    },
    "@rushstack/heft": {
      "injected": true
    },
    "local-node-rig": {
      "injected": true
    }
  },
  "exports": {
    "./lib/*": {
      "types": "./lib-dts/*.d.ts",
      "import": "./lib-esm/*.js",
      "require": "./lib-commonjs/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib-dts/*"
      ]
    }
  }
}
