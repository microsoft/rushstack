{
  "name": "@rushstack/heft-serverless-stack-plugin",
  "version": "1.2.0",
  "description": "Heft plugin for building apps using the Serverless Stack (SST) framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/rushstack.git",
    "directory": "heft-plugins/heft-serverless-stack-plugin"
  },
  "license": "MIT",
  "scripts": {
    "build": "heft build --clean",
    "start": "heft test --clean --watch",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:test": "heft run --only test -- --clean"
  },
  "peerDependencies": {
    "@rushstack/heft": "^1.2.0"
  },
  "dependencies": {
    "@rushstack/node-core-library": "workspace:*"
  },
  "devDependencies": {
    "@rushstack/heft": "workspace:*",
    "@rushstack/heft-webpack4-plugin": "workspace:*",
    "@rushstack/heft-webpack5-plugin": "workspace:*",
    "eslint": "~9.37.0",
    "local-node-rig": "workspace:*"
  },
  "exports": {
    "./lib/*": {
      "types": "./lib-dts/*.d.ts",
      "import": "./lib-esm/*.js",
      "require": "./lib-commonjs/*.js"
    },
    "./heft-plugin.json": "./heft-plugin.json",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib-dts/*"
      ]
    }
  },
  "sideEffects": false
}
