{
  "name": "@rushstack/problem-matcher",
  "version": "0.2.1",
  "description": "A library for parsing VS Code style problem matchers",
  "main": "./lib-commonjs/index.js",
  "module": "./lib-esm/index.js",
  "types": "./dist/problem-matcher.d.ts",
  "exports": {
    ".": {
      "types": "./dist/problem-matcher.d.ts",
      "import": "./lib-esm/index.js",
      "require": "./lib-commonjs/index.js"
    },
    "./lib/*": {
      "types": "./lib-dts/*.d.ts",
      "import": "./lib-esm/*.js",
      "require": "./lib-commonjs/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib-dts/*"
      ]
    }
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/microsoft/rushstack.git",
    "type": "git",
    "directory": "libraries/problem-matcher"
  },
  "scripts": {
    "build": "heft build --clean",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:test": "heft run --only test -- --clean"
  },
  "devDependencies": {
    "@rushstack/heft": "1.2.5",
    "decoupled-local-node-rig": "workspace:*",
    "eslint": "~9.37.0"
  },
  "peerDependencies": {
    "@types/node": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "sideEffects": false
}
