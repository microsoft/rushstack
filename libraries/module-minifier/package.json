{
  "name": "@rushstack/module-minifier",
  "version": "0.7.18",
  "description": "Wrapper for terser to support bulk parallel minification.",
  "main": "lib-commonjs/index.js",
  "module": "lib-esm/index.js",
  "typings": "dist/module-minifier.d.ts",
  "exports": {
    ".": {
      "import": "./lib-esm/index.js",
      "require": "./lib-commonjs/index.js",
      "types": "./dist/module-minifier.d.ts"
    },
    "./lib/*": {
      "import": "./lib-esm/*.js",
      "require": "./lib-commonjs/*.js",
      "types": "./lib-dts/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib-dts/*"
      ]
    }
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/microsoft/rushstack.git",
    "type": "git",
    "directory": "libraries/module-minifier"
  },
  "scripts": {
    "build": "heft build --clean",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:test": "heft run --only test -- --clean"
  },
  "dependencies": {
    "@rushstack/worker-pool": "workspace:*",
    "serialize-javascript": "6.0.2",
    "source-map": "~0.7.3",
    "terser": "^5.9.0"
  },
  "devDependencies": {
    "@rushstack/heft": "workspace:*",
    "@types/node": "20.17.19",
    "@types/serialize-javascript": "5.0.2",
    "local-node-rig": "workspace:*"
  },
  "peerDependencies": {
    "@types/node": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  }
}
