{
  "name": "@rushstack/debug-certificate-manager",
  "version": "1.4.32",
  "description": "Cross-platform functionality to create debug ssl certificates.",
  "main": "lib-commonjs/index.js",
  "module": "lib-esm/index.js",
  "typings": "dist/debug-certificate-manager.d.ts",
  "exports": {
    ".": {
      "import": "./lib-esm/index.js",
      "require": "./lib-commonjs/index.js",
      "types": "./dist/debug-certificate-manager.d.ts"
    },
    "./lib/*": {
      "import": "./lib-esm/*.js",
      "require": "./lib-commonjs/*.js",
      "types": "./lib-dts/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "lib-dts/*"
      ]
    }
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/microsoft/rushstack.git",
    "type": "git",
    "directory": "libraries/debug-certificate-manager"
  },
  "scripts": {
    "build": "heft build --clean",
    "_phase:build": "heft run --only build -- --clean"
  },
  "dependencies": {
    "@rushstack/node-core-library": "workspace:*",
    "@rushstack/terminal": "workspace:*",
    "node-forge": "~1.3.1",
    "sudo": "~1.0.3"
  },
  "devDependencies": {
    "@rushstack/heft": "workspace:*",
    "@types/node-forge": "1.0.4",
    "local-node-rig": "workspace:*"
  }
}
