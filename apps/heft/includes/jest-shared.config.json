{
  "silent": false,

  "rootDir": "../../../../",
  "testURL": "http://localhost/",
  "testMatch": ["<rootDir>/src/**/*.test.{ts,tsx}"],
  "transformIgnorePatterns": [],
  "transform": {
    "\\.(ts|tsx)$": "<rootDir>/node_modules/@rushstack/heft/lib/exports/jest-build-transform",
