  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "All Jest tests",
      "program": "${workspaceFolder}/node_modules/@rushstack/heft/lib/start.js",
      "cwd": "${workspaceFolder}",
      "args": ["--debug", "test", "--clean"],
      "console": "integratedTerminal",
      "sourceMaps": true
    },
     {
      "type": "node",
      "request": "launch",
      "name": "Single Jest test",
      "program": "${workspaceFolder}/node_modules/@rushstack/heft/lib/start.js",
      "cwd": "${workspaceFolder}",
      "args": ["--debug", "test", "--clean", "-u", "--test-path-pattern", "lfxGraphLoader60"],
      "console": "integratedTerminal",
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run LAST BUILD in specified folder",
      "program": "${workspaceFolder}/lib/start-explorer.js",
      "cwd": "(your project path)",
      //"cwd": "C:\\Git\\lockfile-explorer-demos",
      "args": ["--debug"],
      "sourceMaps": true
    }
  ]
}
