{
  // ("compiler" is required)

  "project": {
    // ("entryPointSourceFile" is required)

    "externalJsonFileFolders": [ ]
  },

  "policies": {
    "namespaceSupport": "conservative"
  },

  "validationRules": {
    "missingReleaseTags": "error"
  },

  "apiReviewFile": {
    "enabled": true,
    "apiReviewFolder": "./etc",
    "tempFolder": "./temp"
  },

  "apiJsonFile": {
    "enabled": true,
    "outputFolder": "./dist"
  },

  "packageTypings": {
    "enabled": false,
    "outputFolder": "./dist",
    "dtsFilePathForInternal": "index-internal.d.ts",
    "dtsFilePathForPreview": "index-preview.d.ts",
    "dtsFilePathForPublic": "index-public.d.ts"
  }
}
