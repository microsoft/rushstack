{
  "$schema": "../../../../schemas/api-extractor.schema.json",

  "mainEntryPointFilePath": "index.d.ts",

  "apiReport": {
    "enabled": true,
    "tagsToReport": {
      "@validTag1": true, // Valid custom tag
