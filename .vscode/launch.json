{
	"version": "0.2.0",
	"configurations": [
	{
		"type": "node",
		"request": "launch",
		"name": "documenter",
		"program": "${workspaceFolder}/apps/api-documenter/bin/api-documenter",
		"args": [
			"markdown",
			"--input",
			"/Users/feiyangc/Projects/firebase-js-sdk/packages-exp/app-exp/temp",
			"--output",
			"/Users/feiyangc/Projects/firebase-js-sdk/packages-exp/app-exp/docs"
		],
		"skipFiles": [
			"<node_internals>/**"
		]
	},
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/common/node_modules/gulp/bin/gulp.js",
			"stopOnEntry": false,
			"args": [
			],
			"cwd": "${workspaceRoot}/gulp-core-build",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Rush Debug",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/apps/rush-lib/lib/start.js",
			"stopOnEntry": true,
			"args": [
				"update",
				"--variant",
				"test"
			],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy",
        "--debug"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"sourceMaps": true
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}
