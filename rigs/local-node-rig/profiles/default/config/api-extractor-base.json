{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",

  "mainEntryPointFilePath": "<projectFolder>/lib-dts/index.d.ts",

  "apiReport": {
    "enabled": true,
    "reportFolder": "<projectFolder>/../../common/reviews/api"
  },

  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<projectFolder>/../../common/temp/api/<unscopedPackageName>.api.json"
  },

  "dtsRollup": {
    "enabled": true
  }
}
